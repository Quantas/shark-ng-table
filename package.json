{
  "name": "shark-ng-table",
  "version": "1.1.0",
  "description": "A robust table for Angular built with Accessibility in mind.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/ && rimraf dist.tgz",
    "minify:css": "uglifycss --output dist/shark-ng-table.min.css --cute-comments src/app/table/style/shark-ng-table.css",
    "packagr": "ng-packagr -p ng-package.json",
    "copy": "copyfiles README.md dist/ && copyfiles -f src/app/table/style/shark-ng-table.css dist/ && copyfiles -f src/app/table/style/fonts/* dist/fonts/",
    "build": "npm run clean && npm run test && npm run build:package",
    "build:notest": "npm run clean && npm run build:package",
    "build:package": "npm run packagr && npm run copy && npm run minify:css",
    "samples": "rimraf docs/ && ng build --prod --output-path docs/ --base-href /shark-ng-table/",
    "start": "ng serve",
    "test": "ng test --code-coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Quantas/shark-ng-table.git"
  },
  "author": "Andrew Landsverk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Quantas/shark-ng-table/issues"
  },
  "homepage": "https://github.com/Quantas/shark-ng-table",
  "peerDependencies": {
    "@angular/common": "^5.0.0 || ^6.0.0",
    "@angular/core": "^5.0.0 || ^6.0.0",
    "@angular/forms": "^5.0.0 || ^6.0.0",
    "@angular/router": "^5.0.0 || ^6.0.0",
    "core-js": ">= 2.4.0",
    "rxjs": ">= 5.4.0"
  },
  "devDependencies": {
    "@angular/animations": "5.2.4",
    "@angular/cdk": "5.2.4",
    "@angular/cli": "1.6.5",
    "@angular/common": "5.2.4",
    "@angular/compiler": "5.2.4",
    "@angular/compiler-cli": "5.2.4",
    "@angular/core": "5.2.4",
    "@angular/flex-layout": "^5.0.0-beta.15",
    "@angular/forms": "5.2.4",
    "@angular/http": "5.2.4",
    "@angular/language-service": "5.2.4",
    "@angular/material": "5.2.4",
    "@angular/platform-browser": "5.2.4",
    "@angular/platform-browser-dynamic": "5.2.4",
    "@angular/router": "5.2.4",
    "@types/jasmine": "2.8.3",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "6.0.60",
    "angular2-highlight-js": "^6.0.1-alpha",
    "axe-core": "^2.6.1",
    "codelyzer": "4.0.2",
    "conventional-changelog-cli": "1.3.5",
    "copyfiles": "^2.0.0",
    "core-js": "2.4.1",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-coveralls": "1.1.2",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-junit-reporter": "1.2.0",
    "karma-phantomjs-launcher": "1.0.4",
    "ng-packagr": "2.4.2",
    "protractor": "5.1.2",
    "rxjs": "5.5.6",
    "ts-node": "3.2.0",
    "tslint": "5.8.0",
    "typescript": "2.4.2",
    "uglifycss": "0.0.29",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.19"
  }
}
